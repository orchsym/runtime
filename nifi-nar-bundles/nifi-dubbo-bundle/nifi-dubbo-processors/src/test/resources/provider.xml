<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="dubbo-it-provider" />
	<dubbo:registry address="zookeeper://127.0.0.1:2181"
		username="user" password="pass" />
	<dubbo:protocol name="dubbo" port="20880" />
	<dubbo:service
		interface="com.baishancloud.orchsym.processors.dubbo.it.PersonService"
		ref="personService" protocol="dubbo" timeout="1200000" version="1.2.3" />

	<bean id="personService"
		class="com.baishancloud.orchsym.processors.dubbo.it.PersonServiceImpl" />
</beans>